<div align="left">
<%= link_to "Add Record in Get",new_get_path, class:"btn btn-primary btn-block" %>
<table class="table table-bordered table-striped">
<thead>
<tr>
<th>#</th>
<th>Delivery No</th>
<th>Outlet No/ID </th>
<th>Operations </th>
</tr>
</thead>
<tbody>
<% @gets.each do |get| %>
<tr>
<th><%=get.id%></th>
<td><%= get.delivery_id %></td>
<td><%= get.outlet_id %></td>
<td><div class:"btn-group">
<%= link_to"Edit", edit_get_path(get.id), class:"btn btn-warning"%>
<%= link_to"Show", get_path(get.id), class:"btn btn-info"%>
<%= link_to"delete", get_path(get.id),method: :delete , data: {confirm: "Are You Sure Want to delete?"} ,class:"btn btn-danger"%>
</div></td>
</tr>
<% end %>
</tbody>
</table>
</div>
